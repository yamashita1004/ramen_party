<h1>Public::Ramens#new</h1>
<p>Find me in app/views/public/ramens/new.html.erb</p>
<h1>新規投稿</h1>
<%= form_with model: @ramen, url: ramens_path do |f| %>
 <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>店名</h4>
  <%= f.text_field :name %>

  <h4>紹介文</h4>
  <%= f.text_area :introduction %>
  <h4>住所</h4>
  <%= f.text_field :addresses %>
  <h4>ジャンル</h4>


  <%= f.submit '投稿' %>
<% end %>